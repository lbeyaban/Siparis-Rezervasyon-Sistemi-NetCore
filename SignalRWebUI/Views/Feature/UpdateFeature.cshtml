@model SignalRWebUI.Dtos.FeatureDto.UpdateFeatureDto
@{
    ViewData["Title"] = "UpdateFeature";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Feature Page</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">Update Feature</div>
						</div>
						<div class="card-body">
							<input type="hidden" class="form-control" asp-for="FeatureID" id="title1">
							<div class="form-group">
								<label for="title1">Title 1</label>
								<input type="text" class="form-control" asp-for="Title1" id="title1">
							</div>
							<div class="form-group">
								<label for="description1">Description 1</label>
								<input type="text" class="form-control" asp-for="Description1" id="description1">
							</div>
							<div class="form-group">
								<label for="title2">Title 2</label>
								<input type="text" class="form-control" asp-for="Title2" id="title2">
							</div>
							<div class="form-group">
								<label for="description2">Description 2</label>
								<input type="text" class="form-control" asp-for="Description2" id="description2">
							</div>
							<div class="form-group">
								<label for="title3">Title 3</label>
								<input type="text" class="form-control" asp-for="Title3" id="title3">
							</div>
							<div class="form-group">
								<label for="description3">Description 3</label>
								<input type="text" class="form-control" asp-for="Description3" id="description3">
							</div>
						</div>
						<div class="card-action">
							<input type="submit" class="btn btn-primary" value="Update Feature"></input>
						</div>
					</div>
				</form>
			</div>

		</div>
	</div>
</div>

